      ratio:r=-65.0
      ratio:r=0.0
      ratio:r=26.0
    }
  }

  suspension{
    suspensionOffsets:p3=-0.15, -0.12, 0.12
    defaultGearRadius:r=0.372
    defaultDampeningForce:p2=2500.0, 2500.0
