    wheel_l_01:r=0.39
    wheel_r_01:r=0.39
    wheel_l_02:r=0.15
    wheel_r_02:r=0.15
    recalcWtms:b=yes

    steeringDeflection{
      wheel_l_02:r=-65.0
      wheel_r_02:r=-65.0
    }

    driven{
      wheel_l_01:b=yes
      wheel_r_01:b=yes
    }
  }
}

