    part:t="gun_barrel_02"
    part:t="gun_barrel_03"
    part:t="gun_barrel_04"
    part:t="gun_barrel_05"
    part:t="track_r"
    part:t="track_l"
    part:t="track_r_01"
    part:t="track_l_01"
    part:t="wheel_l_back"
    part:t="wheel_l_front"
    part:t="wheel_l_drive"
    part:t="wheel_r_back"
    part:t="wheel_r_front"
    part:t="wheel_r_drive"
