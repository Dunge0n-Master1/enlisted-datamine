server_no_players_timer{
  _singleton:b=yes

  "server_no_players_timer__handle:u64"{
    value:i64=0
    _tags:t="server"
  }
}

server_explosions{
  _singleton:b=yes

  _group{
    _tags:t="server"
    server_explosions__projectilesCounter:i=0
    server_explosions__maxProjectilesCount:i=1
    server_explosions__maxDelayedFramesCounter:i=10
  }
}

random_low_tickrate{
  _singleton:b=yes

  _group{
    _tags:t="server"
    random_low_tickrate__loweredTickrate:i=15
    random_low_tickrate__actionPeriod:r=180.0
    random_low_tickrate__chanceToLower:r=0.067
    random_low_tickrate__chanceToReset:r=1.0
    random_low_tickrate__currentDeadline:r=0.0
    random_low_tickrate__currentlyLow:b=no
  }
}