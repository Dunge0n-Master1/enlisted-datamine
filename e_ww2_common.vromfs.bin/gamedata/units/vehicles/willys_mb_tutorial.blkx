DamageEffects{

  part{
    name:t="engine"

    onKill{
      fire:r=1.0
      fHitCritical:b=yes
    }
  }

  part{
    name:t="body"

    onKill{
      fire:r=1.0
      fHitCritical:b=yes
    }
  }
}