    }
  }

  engine{
    hp:r=10.0

    engine{
      createSecondaryShatters:b=no
      armorThickness:r=0.3
      armorThrough:r=10.0
      fireProtectionHp:r=100.0
    }
  }

  transmission{
    hp:r=10.0

    transmission{
      createSecondaryShatters:b=no
      armorThickness:r=0.3
      armorThrough:r=10.0
    }
  }
