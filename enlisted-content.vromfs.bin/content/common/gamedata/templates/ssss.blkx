ssss{
  _singleton:b=yes
  _tags:t="render"
  _extends:t="replicating"
  ssss__enabled:b=no
  ssss__shader_name:t="ssss_reflectance_blur_ps"

  "ssss__manager:SeparableSubSurfaceScattering"{
  }

  "ssss__shader:PostFxRenderer"{
  }

  _group{
    _track:b=yes
    transmittance__thickness_bias:r=0.0
    transmittance__thickness_min:r=0.001
    transmittance__thickness_scale:r=110.0
    transmittance__translucency_scale:r=0.99
    transmittance__blur_scale:r=0.003
    transmittance__sample_count:i=1
    reflectance__blur_width:r=0.008
    reflectance__blur_quality:i=1
    reflectance__blur_follow_surface:b=no
  }
}