repeat{
  name:t="squad_member"

  sequencer{

    onceUntilReset{
    }

    setIntVar{
      to:t="isUnreachable"
      value:i=0
    }

    setIntVar{
      to:t="minStance"
      value:i=0
    }

    stopAim{
    }

    success{

      sequencer{

        isAgentPlayerBot{
        }

        setIntVar{
          to:t="minStance"
          value:i=1
        }
      }
    }

    success{

      sequencer{

        findWeaponSlotWithTag{
          tag:t="buildingTool"
          toParam:t="buildToolSlot"
        }

        isWeaponSlotSelected{
          slotParam:t="buildToolSlot"
        }

        chooseBestWeapon{
        }
      }
    }
  }

  subTree{
    name:t=""
    refId:t="behaviour"
  }
}