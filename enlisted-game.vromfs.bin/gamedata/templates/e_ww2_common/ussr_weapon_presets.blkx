ussr_light_assault_preset{
  weaponPreset:t="Light assault"

  "human_weap__weapTemplates:object"{
    primary:t="ppd_3438_gun"
    secondary:t=""
    tertiary:t="tt_33_gun"
    melee:t="knife_weapon"
    grenade:t="grenade_thrower"
  }

  "human_weap__weapInfo:array"{

    "weapInfo:object"{
      reserveAmmoTemplate:t="ppsh_drum"
      numReserveAmmo:i=3
    }

    "weapInfo:object"{
    }

    "weapInfo:object"{
      reserveAmmoTemplate:t="tt_magazine"
      numReserveAmmo:i=3
    }

    "weapInfo:object"{
      reserveAmmoTemplate:t=""
      numReserveAmmo:i=0
    }

    "weapInfo:object"{
      reserveAmmoTemplate:t="mk2_grenade_item"
      numReserveAmmo:i=1
    }
  }

  _group{
    _tags:t="server"

    "human_weap__template_deps:ecs::TemplatesListToInstantiate"{
    }

    "human_weap__template_deps_list:list<t>"{
      tmpl:t="ppsh_drum"
      tmpl:t="tt_magazine"
      tmpl:t="mk2_grenade_item"
    }
  }
}

