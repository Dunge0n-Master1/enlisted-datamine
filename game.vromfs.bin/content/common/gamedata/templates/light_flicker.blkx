light_flicker{
  light_flicker__name:t="undefined"

  _group{
    _replicate:b=yes
    light_flicker__attack_time:p2=0.01, 0.4
    light_flicker__release_time:p2=0.01, 0.1
    light_flicker__on_brightness:p2=0.7, 1.0
    light_flicker__off_brightness:p2=0.0, 0.3
    light_flicker__on_time:p2=0.2, 3.0
    light_flicker__off_time:p2=0.1, 0.2
    light_flicker__noise_brightness_on:p2=0.7, 1.0
    light_flicker__noise_brightness_off:p2=0.7, 1.0
    light_flicker__noise_time:r=10.0
    light_flicker__time:r=0.0
    light_flicker__seed:i=0
    light_flicker__brightness:r=0.0
  }
}

light_flicker_updater{
  _tracked:t="light_flicker_updater__template"
  light_flicker_updater__template:t=""
  light_flicker_updater__color_full_brightness:p4=1.0, 1.0, 1.0, 1.0

  "light_flicker_updater__parentEid:eid"{
  }
}

light_flicker_updater_light{
  _extends:t="light_flicker_updater"
}

light_flicker_updater_mesh{
  _extends:t="light_flicker_updater"
  _extends:t="animchar"
  _extends:t="dynmat_params_base"
  animchar__res:t=""

  "dynamic_material_channels_arr:array"{
    _tags:t="render"

    "dynamic_material_channel:object"{
      dynamic_material_type:t="emissive"
      dynmat_param__emissive_color:p4=1.0, 1.0, 1.0, 10.0
      dynmat_param__emission_albedo_mult:r=0.1
    }
  }
}