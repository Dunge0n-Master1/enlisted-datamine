ssss{
  _singleton:b=yes
  _tags:t="render"
  ssss__shader_name:t="ssss_reflectance_blur_ps"
  ssss__shader_is_optional:b=yes

  "ssss__manager:SeparableSubSurfaceScattering"{
  }

  "ssss__shader:PostFxRenderer"{
  }

  _group{
    _track:b=yes
    transmittance__thickness_bias:r=0.007
    transmittance__thickness_min:r=0.001
    transmittance__thickness_scale:r=110.0
    transmittance__translucency_scale:r=0.89
    transmittance__blur_scale:r=0.01
    transmittance__sample_count:i=3
    transmittance__normal_offset:r=0.015
    reflectance__blur_width:r=0.004
    reflectance__blur_quality:i=1
    reflectance__blur_follow_surface:b=yes
  }
}