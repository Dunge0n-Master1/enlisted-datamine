projectile_sound{

  _group{
    _tags:t="sound"
    projectile_sound__hit:t="auto_select"
    projectile_sound__explode:t="auto_select"
  }
}

projectile_sound_physmat{
  _extends:t="sound_physmat"

  _group{
    _tags:t="sound"

    "sound_physmat__soundMaterials:list<t>"{
      mat:t="dirt"
      mat:t="concrete"
      mat:t="water"
      mat:t="wood"
      mat:t="metal"
    }

    "sound_physmat__physmatToSoundmat:object"{
      concrete:t="concrete"
      stone:t="concrete"
      buildings:t="concrete"
      rocks:t="concrete"
      rocksSlippery:t="concrete"
      gravel:t="concrete"
      ceramic:t="concrete"
      water:t="water"
      water3d:t="water"
      waterPuddle:t="water"
      wood:t="wood"
      wood_railway:t="wood"
      window_wood:t="wood"
      cactus:t="wood"
      softWoodDoor:t="wood"
      metal:t="metal"
      steel:t="metal"
      train:t="metal"
      softSteelDoor:t="metal"
    }
  }

  "projectileSoundPhysmat:tag"{
  }
}

