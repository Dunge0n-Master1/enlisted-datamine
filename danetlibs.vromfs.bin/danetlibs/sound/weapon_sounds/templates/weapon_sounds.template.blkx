      mat:t="water"
      mat:t="wood"
      mat:t="metal"
      mat:t="cactus"
    }

    "sound_physmat__physmatToSoundmat:object"{
      concrete:t="concrete"
      stone:t="concrete"
      buildings:t="concrete"
      rocks:t="concrete"
      rocksSlippery:t="concrete"
      gravel:t="concrete"
      ceramic:t="concrete"
      water:t="water"
      water3d:t="water"
      waterPuddle:t="water"
      wood:t="wood"
      wood_railway:t="wood"
      window_wood:t="wood"
      cactus:t="wood"
      softWoodDoor:t="wood"
      metal:t="metal"
      steel:t="metal"
      train:t="metal"
      softSteelDoor:t="metal"
      flesh:t=""
    }
  }

  "humanMeleeActionSoundPhysmat:tag"{
  }
}