motion_matching_trajectory{
  _tags:t="render"
  mm_trajectory__historyLength:i=0
  mm_trajectory__lastHistoryIdx:i=-1
  mm_trajectory__lastHistoryTime:r=0.0
  mm_trajectory__historyDt:r=0.0
  mm_trajectory__prevPosition:p3=0.0, 0.0, 0.0
  mm_trajectory__prevRotation:p4=0.0, 0.0, 0.0, 1.0
  mm_trajectory__currPosition:p3=0.0, 0.0, 0.0
  mm_trajectory__currRotation:p4=0.0, 0.0, 0.0, 1.0
  mm_trajectory__linearVelocity:p3=0.0, 0.0, 0.0
  mm_trajectory__angularVelocity:p3=0.0, 0.0, 0.0
  mm_trajectory__linearVelocityViscosity:r=0.1
  mm_trajectory__angularVelocityViscosity:r=0.1

  "mm_trajectory__historyPositions:list<p3>"{
  }

  "mm_trajectory__historyRotations:list<p4>"{
  }

  "mm_trajectory__featurePositions:list<p3>"{
  }

  "mm_trajectory__featureDirections:list<p3>"{
  }
}

motion_matching_animchar_component{
  _extends:t="motion_matching_trajectory"
  _tags:t="render"
  motion_matching__enabled:b=no
  motion_matching__blendTimeToAnimtree:r=0.45
  motion_matching__metricaTolerance:r=0.0
  motion_matching__animationBlendTime:r=0.45
  motion_matching__updateProgress:r=0.0
  motion_matching__distanceFactor:r=1.0
  motion_matching__trajectoryTolerance:r=0.0
  motion_matching__presetIdx:i=-1
  motion_matching__dataBaseTemplateName:t="motion_matching_animation_database"

  "motion_matching__goalFeature:FrameFeatures"{
  }

  "motion_matching__goalNodesIdx:list<i>"{
  }

  "motion_matching__goalTags:list<t>"{
  }

  "motion_matching__optionalTags:list<t>"{
  }

  "motion_matching__currentTags:Tags"{
  }

  "motion_matching__dataBaseEid:eid"{
  }

  "motion_matching__controller:MotionMatchingController"{
  }

  _group{
    _tags:t="tools"
    motion_matching__walkSpeed:r=1.8
    motion_matching__runSpeed:r=3.7
  }
}

motion_matching_foot_ik_locker{
  _tags:t="render"
  foot_ik_locker_inited:b=no
  foot_ik_locker_enabled:b=yes
  foot_ik_locker_blendTime:r=0.2
  foot_ik_locker_maxReachScale:r=0.99
  foot_ik_locker_unlockByAnimRadius:r=0.2
  foot_ik_locker_unlockWhenUnreachableRadius:r=0.05

  "foot_ik_locker_nodes:FootIKLockerNodes"{
  }

  "foot_ik_locker_nodeNames:array"{

    "leg:object"{
      toeNodeName:t="Bip01 L Toe0"
      heelNodeName:t="Bip01 L Foot"
      kneeNodeName:t="Bip01 L Calf"
      hipNodeName:t="Bip01 L Thigh"
    }

    "leg:object"{
      toeNodeName:t="Bip01 R Toe0"
      heelNodeName:t="Bip01 R Foot"
      kneeNodeName:t="Bip01 R Calf"
      hipNodeName:t="Bip01 R Thigh"
    }
  }
}

motion_matching_animation_database{
  _singleton:b=yes
  _tags:t="render"
  main_database__loaded:b=no
  main_database__perFrameLimit:i=-1
  weightsPath:t="%danetlibs/motion_matching/animation_data_base/main_data_base_weights.blk"
  rootMotionPath:t="%danetlibs/motion_matching/animation_data_base/main_data_base_root_motion.blk"
  main_database__nodeMasksPath:t="%danetlibs/motion_matching/animation_data_base/main_data_base_node_masks.blk"
