soldier_mm_database{
  _extends:t="motion_matching_animation_database"
  _singleton:b=yes
  _tags:t="render"
  weightsPath:t="%danetlibs/motion_matching/animation_data_base/soldier_mm_weights.blk"
  rootMotionPath:t="%danetlibs/motion_matching/animation_data_base/soldier_mm_root_motion.blk"

  "data_bases_paths:list<t>"{
