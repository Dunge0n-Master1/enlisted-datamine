      type:t="apexDestr"
      type:t="fx"
      type:t="lshader"
      type:t="impostorData"
    }

    plugins{
      folder:t="*common"
    }

    folderDefaults{
      ddsxTexPack:t="*parent"
      gameResPack:t="*parent"
    }
  }

  build{

