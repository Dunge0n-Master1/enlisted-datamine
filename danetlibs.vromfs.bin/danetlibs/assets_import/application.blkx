    dynModel{
      descListOutPath:t="dynModelDesc"
      separateModelMatToDescBin:b=yes
      ignoreMappingInPrepareBillboardMesh:b=yes
      enableMeshNodeCollapse:b=no
      maxBonesCount:i=256
      setBonePerVertex:i=4
